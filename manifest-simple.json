{
  "manifest_version": 3,
  "name": "DubFusion Test",
  "version": "0.1.0",
  "description": "Simple test version of DubFusion",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://music.youtube.com/*",
        "file://*",
        "http://localhost:*"
      ],
      "js": ["content-script-simple.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_title": "DubFusion Test"
  }
}
